{
  "name": "maillauncher/maillauncher-php-sdk",
  "type": "library",
  "description": "MailLauncher PHP-SDK",
  "keywords": ["MailLauncher"],
  "homepage": "https://www.maillauncher.com",
  "license": "MIT",
  "authors": [
    {
      "name": "maillauncher",
      "email": "support@maillauncher.com"
    }
  ],
  "require": {
    "php": ">=5.2"
  },
  "autoload": {
    "files": ["MailLauncherApi/autoload.php"]
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.9",
    "phpunit/phpunit": "^7.5",
    "friendsofphp/php-cs-fixer": "^2.15"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "fix-style": "./vendor/bin/php-cs-fixer fix .",
    "analyse": "./vendor/bin/phpstan analyse . -c phpstan.neon --level max --memory-limit=-1"
  },
  "extra": {
    "branch-alias": {
        "dev-master": "1.0.7-dev"
    }
  }
}
